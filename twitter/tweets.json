{
    "version": 1,
    "request": {
        "url": "https://api.twitter.com/1.1/search/tweets.json",
        "method": "GET",
        "authorization": {
            "type": "OAuth2",
            "grantType": "ClientCredentials",
            "tokenServerUrl": "https://api.twitter.com/oauth2/token"
        },
        "params": {
            "q": false,
            "count": false,
            "max_id": false
        },
        "pagination":{
            "type":"cursor",
            "cursor_id":"id",
            "cursor_key":"max_id",
            "count_key":"count",
            "max_count": 100
        }
    },
    "response": {
        "ctype": "application/json",
        "tablePath": "$.statuses[*]",
        "schema": {
            "created_at": {
                "target": "$.created_at",
                "type": "string"
            },
            "text":{
                "target": "$.text",
                "type": "string"
            },
            "truncated":{
                "target": "$.truncated",
                "type": "boolean"
            },
             "iso_language_code":{
                "target": "$.metadata.iso_language_code",
                "type": "string"
            },
            "hashtags":{
                "target": "$.entities.hashtags",
                "type": "string"
            }
        },
        "orient": "records"
    }
}